        </LinearLayout>
    </ScrollView>

    <TextView
        android:id="@+id/textView48"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="@drawable/blk_border_with_radius"
        android:text="A Treatment consist of one of the 3 Main Treatment Packages listed below with the option for Add-on Treatment Packages. Please note the Add-ons Treatment Packages are not stand alone packages and must be paired with a Main Treatment Package."
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/scrollView2"
        app:layout_constraintStart_toStartOf="@+id/scrollView2"
        app:layout_constraintTop_toBottomOf="@+id/tv_vPackageDetails_headerText" />

</androidx.constraintlayout.widget.ConstraintLayout>